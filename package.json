{
  "name": "backend-dev-test",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:int": "vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "local:up": "docker compose up -d",
    "local:down": "docker compose down",
    "local:deploy": "CDK_DISABLE_LEGACY_EXPORT_WARNING=1 cdklocal bootstrap && CDK_DISABLE_LEGACY_EXPORT_WARNING=1 cdklocal deploy --all --require-approval never",
    "local:destroy": "CDK_DISABLE_LEGACY_EXPORT_WARNING=1 cdklocal destroy --all --force",
    "local:reset": "docker compose down -v && docker compose up -d && npm run local:deploy",
    "local:logs": "docker compose logs -f localstack",
    "upload": "IS_LOCAL=true npx tsx src/scripts/upload.ts",
    "retrieve": "IS_LOCAL=true npx tsx src/scripts/retrieve.ts",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy --all"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.0",
    "@aws-sdk/client-api-gateway": "^3.990.0",
    "@aws-sdk/client-cloudformation": "^3.990.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "aws-cdk-lib": "^2.170.0",
    "constructs": "^10.4.2",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@aws-sdk/util-stream-node": "^3.370.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.0",
    "@vitest/coverage-v8": "^3.0.0",
    "aws-cdk": "^2.170.0",
    "aws-sdk-client-mock": "^4.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
